<?xml version="1.0" encoding="UTF-8" ?>
<LoginWindow>
	<Version>1.0</Version>
	<WinHeight>433</WinHeight>
	<WinWidth>290</WinWidth>
	<Layout>
				<VBoxLayout Name="LoginRoot" SizePolicy="Spring;Spring" Background="#FFFFFF">
					<Space Size="15"/>
					<HBoxLayout Name="LoginCloseRegion" SizePolicy="Spring;Fix" Height="15">
						<Spring/>
						<IconButton Name="LoginClose" SizePolicy="Fix;Fix" Background="#FFFFFF" Width="15" Height="15" IconWidth="11" IconHeight="11" Icon="login-close.png;login-close.png;login-close.png;login-close.png"/>
						<Space Size="15"/>
					</HBoxLayout>
					<HBoxLayout Name="LoginIcon" SizePolicy="Spring;Fix" Height="81">
						<Spring/>
						<IconButton Name="LoginLogo" SizePolicy="Fix;Fix" Background="#FFFFFF" Width="105" Height="81" Icon="noImg;noImg;noImg;login-logo.png" Enabled="False"/>
						<Spring/>
					</HBoxLayout>
					<Space Size="25"/>
					<HBoxLayout Name="LoginName" SizePolicy="Spring;Fix" Height="30">
						<Space Size="30"/>
						<HBoxLayout Name="LoginNameEditRegion" SizePolicy="Spring;Fix" Height="30" Background="login-input.png">
						<Space Size="15"/>
							<LoginCombBox Name="LoginNameEdit" SizePolicy="Spring;Fix" Height="30" MaxTextNum="256"  Background="#00EEEEEE" BackUnfocus="#00F3F3F3" TextColor="#005F6266" DefaultText="@LoginNamePlaceholder" FontFamily="微软雅黑" FontSize="12" DropBgColor="#00FFFFFF" ItemHeight="28" ItemTextColor="#005F6266" ItemBgColor="#00FFFFFF" ItemBgColorFocus="#00ECEEF1" BorderColor="#00D2D2D5"/>
						<Space Size="15"/>
						</HBoxLayout>
						<Space Size="30"/>
					</HBoxLayout>
					<Space Size="10"/>
					<HBoxLayout Name="LoginPW" SizePolicy="Spring;Fix" Height="30">
						<Space Size="30"/>
						<HBoxLayout Name="LoginPWEditRegion" SizePolicy="Spring;Fix" Height="30" Background="login-input.png">
							<Space Size="15"/>
							<InputBox Name="LoginPWEdit" SizePolicy="Spring;Fix" Mode="Password" Height="30" MaxTextNum="256" Background="#00EEEEEE" BackUnfocus="#00F3F3F3" TextColor="#005F6266" DefaultText="@LoginPWPlaceholder" FontFamily="微软雅黑" FontSize="12"/>
							<Space Size="15"/>
						</HBoxLayout>
						<Space Size="30"/>
					</HBoxLayout>
					<Space Size="10"/>
					<HBoxLayout Name="LoginOption" SizePolicy="Spring;Fix" Height="30">
						<Space Size="30"/>
						<CheckBox Name="LoginAutoOpt" SizePolicy="Fix;Fix" Background="#FFFFFF" Height="16" Width="60" Icon="check.png;check-hover.png;check-hover.png;check.png" CheckedIcon="checked.png" IconMargin="0" IconHeight="16" IconWidth="16" Text="@LoginAuto" TextMargin="25"  FontFamily="微软雅黑" FontSize="12" TextColor="#2E3033"/>
						<Spring/>
						<TextButton Name="LoginForgetPW" SizePolicy="Auto;Auto" Background="#FFFFFF" Width="60" Height="30" TextAlign="Center" Text="@LoginForgetPW" FontFamily="微软雅黑" FontSize="12" TextColor="#5F6266;#FFB4B4;#FFB4B4;#5F6266"/>
						<Space Size="30"/>
					</HBoxLayout>
					<Space Size="5"/>
					<TextButton Name="ErrorMsg" SizePolicy="Spring;Fix" Height="12" Background="#FFFFFF" TextAlign="Center" Text="" FontFamily="微软雅黑" FontSize="12" TextColor="#DB3E3E;#DB3E3E;#DB3E3E;#DB3E3E" Enabled="False"/>
					<Space Size="5"/>
					<HBoxLayout Name="LoginSubmitRegion" SizePolicy="Spring;Fix" Height="30">
						<Space Size="30"/>
						<TextButton Name="LoginSubmit" SizePolicy="Spring;Fix" Height="30" Icon="login-button.png;login-button-hover.png;login-button-down.png;login-button.png" IconMode="Stretch" TextAlign="Center" Text="@LoginButtonText" FontFamily="微软雅黑" FontSize="12" TextColor="#FFFFFF;#FFFFFF;#FFFFFF;#FFFFFF"/>
						<Space Size="30"/>
					</HBoxLayout>
					<Space Size="10"/>
					<HBoxLayout Name="LoginRegisterRegion" SizePolicy="Spring;Fix" Height="30">
						<Space Size="30"/>
						<TextButton Name="LoginRegister" SizePolicy="Spring;Fix" Height="30" Icon="login-register.png;login-register-hover.png;login-register-down.png;login-button.png" IconMode="Stretch" TextAlign="Center" Text="@LoginRegisterText" FontFamily="微软雅黑" FontSize="12" TextColor="#2E3033;#2E3033;#2E3033;#2E3033"/>
						<Space Size="30"/>
					</HBoxLayout>
					<VBoxLayout Name="LoginOtherRegion" SizePolicy="Spring;Spring" Background="#F9FBFC">
						<Spring/>
						<Separator SizePolicyH="Spring;Fix" Color="#DDE0E4" Height="1" ForeHeight="1" ForeWidth="290" Background="#F9FBFC"/>
						<Space Size="15"/>
						<TextButton Name="LoginOtherText" SizePolicy="Spring;Fix" Background="#F9FBFC" Height="10" TextAlign="AlignCenter" Text="@LoginOtherLogin" FontFamily="微软雅黑" FontSize="12" TextColor="#0;#0;#0;#2E3033" Enabled="False"/>
						<Space Size="10"/>
						<HBoxLayout Name="LoginOther" SizePolicy="Spring;Fix" Height="20">
							<Spring/>
							<IconButton Name="LoginWeixin" SizePolicy="Fix;Fix" Width="20" Height="20" Background="#F9FBFC" IconWidth="20" IconHeight="15" Icon="weixin.png;weixin-hover.png;weixin-hover.png;weixin.png"/>
							<Space Size="30"/>
							<IconButton Name="LoginWeibo" SizePolicy="Fix;Fix" Width="20" Height="20" Background="#F9FBFC" IconWidth="19" IconHeight="15" Icon="weibo.png;weibo-hover.png;weibo-hover.png;weibo.png"/>
							<Space Size="30"/>
							<IconButton Name="LoginQQ" SizePolicy="Fix;Fix" Width="20" Height="20" Background="#F9FBFC" IconWidth="15" IconHeight="17" Icon="qq.png;qq-hover.png;qq-hover.png;qq.png"/>
							<Spring/>
						</HBoxLayout>
						<Space Size="15"/>
					</VBoxLayout>
				</VBoxLayout>
	</Layout>
</LoginWindow>