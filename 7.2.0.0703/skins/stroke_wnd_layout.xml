<?xml version="1.0" encoding="UTF-8" ?>
<StrokeWnd>
	<Version>1.0</Version>
	<WinHeight>30</WinHeight>
	<WinWidth>330</WinWidth>
	<Layout>
		<VBoxLayout Name="RootLayout" Background="#DB3E3E" SizePolicy="Spring;Spring">
			<HBoxLayout Name="StrokeTitle" SizePolicy="Spring;Fix" Height="30">
				<Space Size="15"/>
				<IconButton Name="StrokeLogo" Background="#DB3E3E" Width="30" Height="16" Icon="noImg;noImg;noImg;qclogo.png" Enabled="False"/>
				<Space Size="5"/>
				<Separator Height="12" Width="1" Background="#FFFFFF"/>
				<Space Size="5"/>
				<TextButton Name="WinName" SizePolicy="Auto;Auto" Text="@StrokeWinTitle" TextColor="#FFFFFF;#FFFFFF;#FFFFFF;#FFFFFF" FontFamily="微软雅黑" FontSize="12" Enabled="False"/>
				<Space Size="15"/>
				<TextButton Name="LangSelector" Text="@LangEng" SizePolicy="Auto;Auto" TextColor="#FFFFFF;#FFFFFF;#FFFFFF;#FFFFFF" FontFamily="微软雅黑" FontSize="12"/>
				<Space Size="2"/>
				<IconButton Name="LangDrop" Background="#DB3E3E" Icon="mini-drop.png;mini-drop.png;mini-drop.png;mini-drop.png" Width="16" Height="16" IconWidth="13" IconHeight="8" Linkage="LangSelector"/>
				<Spring/>
				<IconButton Name="StrokeSetting" Background="#DB3E3E" Icon="qcconfig.png;qcconfig-hover.png;qcconfig-hover.png;qcconfig.png" Width="16" Height="16" Visible="False"/>
				<Space Size="10"/>
				<FlipIconButton Name="StrokeNail" Background="#DB3E3E" Flip="False" FlipIcon="qcfix.png;qcfix-hover.png;qcfix-hover.png;qcfix.png" Icon="qcsprig.png;qcsprig-hover.png;qcsprig-hover.png;qcsprig.png" Width="16" Height="16"/>
				<Space Size="10"/>
				<IconButton Name="StrokeClose" Background="#DB3E3E" Icon="qcclose.png;qcclose-hover.png;qcclose-hover.png;qcclose.png" Width="16" Height="16"/>
				<Space Size="15"/>
			</HBoxLayout>
			<HBoxLayout Name="StrokeContent" SizePolicy="Spring;Spring">
				<Space Size="1"/>
				<CefWnd Name="StrokeResult" WndClass="YdStrokeCefWnd" SizePolicy="Spring;Spring" MinWidth="326" MinHeight="200" StartPage="frame://dict/huaci.html"/>
			    <LangSelect Name="LangSelWnd" SizePolicy="Fix;Fix" Buddy="LangSelector" Float="Bottom" Style="Popup" Distance = "10" Extend="15;0" Height="72" Width="335" Layout="lang_select_layout.xml" Visible="False"/>
				<Space Size="1"/>
			</HBoxLayout>
			<Space Size="1"/>
		</VBoxLayout>
	</Layout>
</StrokeWnd>